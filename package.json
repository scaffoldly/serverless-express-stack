    "@tsoa/cli": "6.x",
    "@types/serve-static": "1.x",
    "aws-lambda": "1.x",
    "axios": "1.x",
    "bs58": "5.x",
    "cookies": "0.x",
    "cors": "2.x",
    "ddb-table": "2.x",
    "express": "4.x",
    "jose": "2.x",
    "mime-types": "2.x",
    "morgan-body": "2.x",
    "multer": "1.4.5-lts.1",
    "path-parser": "6.x",
    "tsoa": "6.x",
    "uuid": "9.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,cjs,ts,tsx}": [
      "pretty-quick --staged",
