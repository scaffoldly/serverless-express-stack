{
  "name": "stack",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "yarn dotenv && yarn openapi && yarn codegen && yarn build",
    "build": "tsc",
    "start": "serverless offline start",
    "deploy": "serverless deploy",
    "serverless": "serverless",
    "dotenv": "dotenv-out -f typescript -e .env -e .scaffoldly/.env -c $NODE_ENV -o src",
    "openapi": "openapi-generator -g axios -i .scaffoldly/$NODE_ENV -o src/services/openapi -r auth-sls-rest-api",
    "codegen": "ts-node codegen/index.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && yarn run prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@scaffoldly/openapi-generator": "^1.0.26",
    "@tsoa/cli": "^5.1.1",
    "@types/aws-lambda": "^8.10.77",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookies": "0.7.7",
    "@types/express": "^4.17.11",
    "@types/multer": "^1.4.7",
    "@types/node": "18",
    "@types/notp": "2.0.1",
    "@types/promise-retry": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "cross-env": "^7.0.3",
    "dotenv-out": "^1.0.6",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^4.0.0",
    "prettier": "^2.4.1",
    "serverless": "3.27.0",
    "serverless-bundle": "^6.0.0",
    "serverless-cloudside-plugin": "^1.0.6",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-resource-tagging": "^1.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0"
  },
  "dependencies": {
    "@octokit/oauth-app": "^6.0.0",
    "@octokit/webhooks": "^12.0.3",
    "@scaffoldly/serverless-util": "^4.0.41",
    "@tsoa/runtime": "^5.0.0",
    "@vendia/serverless-express": "^4.5.2",
    "aws-lambda": "^1.0.6",
    "axios": "^0.21.1",
    "cookies": "0.8.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "joi-to-typescript": "^4.7.0",
    "jose": "2.0.5",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "node-2fa": "2.0.2",
    "octokit": "^3.1.0",
    "promise-retry": "^2.0.1",
    "ulid": "^2.3.0"
  }
}
