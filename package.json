{
  "name": "stack",
  "description": "Serverless Stack",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepare": "husky && yarn dotenv && yarn tsoa",
    "start": "serverless offline",
    "build": "serverless package",
    "tsoa": "serverless tsoa",
    "serverless": "serverless",
    "dotenv": "dotenv-out -f typescript -e .env -e .scaffoldly/.env -c $NODE_ENV -o src",
    "lint": "eslint '*/**/*.{js,cjs,ts,tsx}' --quiet --fix",
    "format": "prettier --write '*/**/*.{js,cjs,ts,tsx}'"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.x",
    "@types/cookies": "0.x",
    "@types/cors": "2.x",
    "@types/express": "4.x",
    "@types/mime-types": "2.x",
    "@types/node": "18.x",
    "@types/react": "18.x",
    "@types/react-dom": "18.x",
    "@types/uuid": "9.x",
    "@typescript-eslint/eslint-plugin": "6.x",
    "@typescript-eslint/parser": "6.x",
    "dotenv-out": "1.x",
    "esbuild": "0.x",
    "eslint": "8.x",
    "eslint-config-airbnb": "19.x",
    "eslint-config-airbnb-typescript": "17.x",
    "eslint-config-prettier": "9.x",
    "eslint-import-resolver-typescript": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "5.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "4.x",
    "husky": "9.x",
    "lint-staged": "15.x",
    "prettier": "3.x",
    "pretty-quick": "4.x",
    "serverless": "3.x",
    "serverless-dotenv-plugin": "6.x",
    "serverless-esbuild": "1.x",
    "serverless-offline": "13.x",
    "serverless-offline-resources": "1.x",
    "serverless-react": "1.x",
    "serverless-tsoa": "1.x",
    "ts-node": "10.x",
    "typescript": "5.x"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.x",
    "@aws-sdk/client-secrets-manager": "3.x",
    "@aws-sdk/client-sns": "3.x",
    "@aws-sdk/client-sqs": "3.x",
    "@aws-sdk/lib-dynamodb": "3.x",
    "@aws-sdk/util-dynamodb": "3.x",
    "@tsoa/cli": "6.x",
    "@types/serve-static": "1.x",
    "@vendia/serverless-express": "4.x",
    "@vitejs/plugin-react": "4.x",
    "aws-lambda": "1.x",
    "axios": "1.x",
    "bs58": "5.x",
    "cookies": "0.x",
    "cors": "2.x",
    "ddb-table": "2.x",
    "dotenv": "16.x",
    "express": "4.x",
    "jose": "2.x",
    "mime-types": "2.x",
    "morgan-body": "2.x",
    "path-parser": "6.x",
    "react": "18.x",
    "react-dom": "18.x",
    "tsoa": "6.x",
    "uuid": "9.x",
    "vite": "5.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,cjs,ts,tsx}": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  }
}
