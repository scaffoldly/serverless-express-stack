{
  "name": "stack",
  "description": "Serverless Stack",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": "true",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "prepare": "yarn dotenv && yarn codegen",
    "start": "yarn prepare && serverless offline",
    "build": "serverless webpack",
    "deploy": "serverless deploy",
    "serverless": "serverless",
    "dotenv": "dotenv-out -f typescript -e .env -e .scaffoldly/.env -c $NODE_ENV -o src",
    "codegen": "ts-node scripts/codegen.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && yarn run prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.77",
    "@types/cookies": "0.7.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/node": "18",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "dotenv": "^16.3.1",
    "dotenv-out": "^1.0.6",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "react-scripts": "^5.0.1",
    "serverless": "3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.50.1",
    "serverless-offline": "^12.0.4",
    "serverless-offline-resources": "^0.1.17",
    "serverless-react": "git+https://github.com/scaffoldly/serverless-react.git#9e7910f47181f0c6440aefe4c35a146378f8a1a6",
    "ts-node": "^10.9.1",
    "typescript": "5.x"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.x",
    "@aws-sdk/client-secrets-manager": "3.x",
    "@aws-sdk/client-sns": "3.x",
    "@aws-sdk/client-sqs": "3.x",
    "@aws-sdk/lib-dynamodb": "3.x",
    "@aws-sdk/util-dynamodb": "3.x",
    "@tsoa/cli": "^6.0.0",
    "@vendia/serverless-express": "^4.5.2",
    "aws-lambda": "^1.0.7",
    "axios": "^1.6.3",
    "bs58": "^5.0.0",
    "cookies": "0.8.0",
    "cors": "^2.8.5",
    "ddb-table": "^2.1.1",
    "express": "^4.17.1",
    "jose": "2.0.5",
    "morgan-body": "2.x",
    "path-parser": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsoa": "^6.0.0",
    "uuid": "9.x"
  }
